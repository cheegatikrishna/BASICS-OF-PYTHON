""""WHILE LOOP"""

n=int(input("enter the value:"))
while n<10:
    print(n,end=" ")
    n+=1
print()

#continue statement
a=1
while a<5:
    a+=1
    if a==4:
        continue
    print(a,end=" ")
print()

#break statement
b=0
while b<10:
    print(b,end=" ")
    if b==4:
        break
    b+=1
print()
